@using Test_Project.Models
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Settings <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            
                            <li>@Html.ActionLink("Area", "TaxArea", "TaxArea")</li>
                            <li>@Html.ActionLink("Investment Rebate Area", "TaxInvestmentRebateArea", "TaxInvestmentRebateArea")</li>
                            <li>@Html.ActionLink("Investment Rebate Slab", "InvestmentRebateSlab", "InvestmentRebateSlab")</li>
                            <li>@Html.ActionLink("Asset Type", "TaxAssetType", "TaxAssetType")</li>
                            <li>@Html.ActionLink("Service Charge Slab", "TaxServiceChargeSlab", "TaxServiceChargeSlab")</li>
                            <li>@Html.ActionLink("Area Wise Minimum Tax", "AreaWiseMinimumTax", "AreaWiseMinimumTax")</li>
                            <li>@Html.ActionLink("Tax Slab", "TaxSlab", "TaxSlab")</li>
                            <li>@Html.ActionLink("Tax Employee Asset", "TaxEmployeeAsset", "TaxEmployeeAsset")</li>
                            <li>@Html.ActionLink("Tax Employee Investment", "TaxEmployeeInvestment", "TaxEmployeeInvestment")</li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @*<hr />*@
        @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@
    </div>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>
